	<queryString>
		<![CDATA[SELECT
@numBus:=IdConductor As 'N° Bus', (SELECT CONCAT(Conductores.Apellidos," ", LEFT(Conductores.Nombres,1),".") FROM Conductores WHERE Codigo = @numBus) AS 'Nombre',
@dt:=COUNT(DISTINCT Fecha) AS 'D.T.',
SUM(TotalIngresos) As 'T.Ingresos',
@liquido:=SUM(Saldo) AS 'Líquido',
SUM(TotalEgresos) AS 'T.Egresos', SUM(EgresosXGuias.Combustible) As Combustible,
@vueltas:=IF(SUM(NumVueltas)=0, 1, SUM(NumVueltas)) AS 'N°Vueltas'
FROM Guias, EgresosXGuias
WHERE Fecha BETWEEN '2009-05-01' AND '2009-05-30' AND Guias.Folio = EgresosXGuias.IdGuia
GROUP BY IdConductor
ORDER BY SUM(Saldo) DESC]]>
	</queryString>
